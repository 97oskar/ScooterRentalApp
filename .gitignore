#System files
api/.DS_Store
web-app/.DS_Store

### Node.js gitignore ###

# Logs
api/logs
api/*.log
api/npm-debug.log*
api/yarn-debug.log*
api/yarn-error.log*

web-app/npm-debug.log*
web-app/yarn-debug.log*
web-app/yarn-error.log*

# Runtime data
api/pids
api/*.pid
api/*.seed
api/*.pid.lock

# Compiled binary addons (https://nodejs.org/api/addons.html)
api/build/Release

# Dependency directories
api/node_modules/
api/jspm_packages/

web-app/node_modules/
web-app//dist/

# Optional npm cache directory
api/.npm

# Optional REPL history
api/.node_repl_history

# Output of 'npm pack'
api/*.tgz

# dotenv environment variables file
api/.env

# parcel-bundler cache (https://parceljs.org/)
api/.cache

# next.js build output
api/.next
